							"});"
						]
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "5e2607f8-f57a-4891-ad5e-6dcc6ba81c38",
						"type": "text/javascript",
						"exec": [
							"let date = Date.now();",
							"",
							"pm.globals.set(\"currentDate\", date);"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"john\",\n    \"permissions\": [2000,3000,4000]\n}"
				},
				"url": {
					"raw": "http://httpbin.org/post",
					"protocol": "http",
					"host": [
						"httpbin",
						"org"
					],
					"path": [
						"post"
					]
				}
			},
			"response": []
		},
		{
